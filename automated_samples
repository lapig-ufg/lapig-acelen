//Import modules and datasets
var func = require('users/Amazonas21/acelen:functions')
var data = require('users/Amazonas21/acelen:datasets')

//Parametros
var type = 'cultivated'
var year = 2001
var area = data.datasets['test_data'].filter(ee.Filter.eq('Fazenda','ADRIANA'))
var imgs = data.datasets[type+"_perc"].toBands()
var numberSamples = 500
var scale = 30


//Calculation automated samples        
var target = func.automatedSamples(imgs,type,year,area,numberSamples,scale)
Map.addLayer(target['target-data'],{palette:['red','green']},'Target Data')
Map.addLayer(target['samples'],{},'Samples')




